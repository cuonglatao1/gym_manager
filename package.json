{
  "name": "gym-manager-simple",
  "version": "1.0.0",
  "description": "Hệ thống quản lý phòng gym đơn giản",
  "main": "app.js",
  "scripts": {
    "seed:membership": "node seeders/membershipSeeder.js",
    "start": "npx kill-port 3000 && node app.js",
    "dev": "npx kill-port 3000 && nodemon app.js",
    "clean-start": "npx kill-port 3000 30001 && node app.js",
    "force-start": "taskkill /f /im node.exe >nul 2>&1 && node app.js",
    "test": "jest",
    "test:watch": "jest --watch", 
    "test:coverage": "jest --coverage",
    "test:all": "node scripts/run-all-tests.js",
    "test:performance": "node scripts/run-all-tests.js --performance",
    "test:security": "npm test tests/security",
    "test:integration": "npm test tests/integration"
  },
  "keywords": [
    "gym",
    "management",
    "nodejs",
    "express",
    "postgresql"
  ],
  "author": "Đặng Đức Cường",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "autocannon": "^8.0.0",
    "clinic": "^13.0.0",
    "eslint-plugin-security": "^3.0.1",
    "helmet": "^8.1.0",
    "jest": "^30.0.5",
    "nodemon": "^3.0.1",
    "rate-limiter-flexible": "^7.2.0",
    "supertest": "^7.1.4"
  }
}
